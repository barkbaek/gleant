<!DOCTYPE html>
<html itemscope itemtype="http://schema.org/Product" lang="{{lang}}">
<head>
    <link rel="stylesheet" type="text/css" href="/css/main.css{{css_version}}">
    <link rel="stylesheet" type="text/css" href="/css/animate.css{{css_version}}">
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta name="fragment" content="!">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1"/>
    <!--[if lte IE 8]>
    <script src="https://cdn.polyfill.io/v2/polyfill.min.js"></script>
    <![endif]-->
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no, height=device-height">
    <meta name="mobile-web-app-capable" content="yes" />
    <meta name="apple-mobile-web-app-capable" content="yes" />
    <meta name="apple-mobile-web-app-status-bar-style" content="black" />
    <meta http-equiv="content-script-type" content="text/javascript">
    <meta name="title" content="{{title}}">
    <meta name="description" content="{{description}}">
    <meta name="kewords" content="{{keywords}}">
    <meta name="author" content="{{title}}" />
    <meta name="robots" content="index,follow" />
    <meta name="google" content="notranslate">
    <meta itemprop="name" content="{{title}}">
    <meta itemprop="description" content="{{description}}">
    <meta itemprop="image" content="{{image}}">
    <meta name="twitter:card" content="summary">
    <meta name="twitter:site" content="{{twitter_site}}">
    <meta name="twitter:title" content="{{title}}">
    <meta name="twitter:description" content="{{description}}">
    <meta name="twitter:creator" content="{{twitter_site}}">
    <meta name="twitter:image" content="{{image}}">
    <meta property="og:site_name" content="{{site_name}}" />
    <meta property="og:title" content="{{title}}" />
    <meta property="og:type" content="article" />
    <meta property="og:url" content="{{url}}" />
    <meta property="og:image" content="{{image}}" />
    <meta property="og:description" content="{{description}}" />
    <meta property="article:published_time" content="{{date}}" />
    <meta property="article:modified_time" content="{{date}}" />
    <meta property="fb:app_id" content="{{fb_app_id}}" />
    <title>{{title}}</title>
    <link rel="icon" sizes="32x32" href="/favicon.ico{{css_version}}">
    {{#each alternate_list}}
        <link rel="alternate" href="{{url}}" hreflang="{{lang}}" />
    {{/each}}
    <script type="text/javascript" src="/js/sub/check.js{{js_version}}"></script>
    <script async src="//pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script>
    <script>
        (adsbygoogle = window.adsbygoogle || []).push({
            google_ad_client: "ca-pub-8698396929836495",
            enable_page_level_ads: true
        });
    </script>
</head>
<body data-lang="{{lang}}" data-check="{{is_loginned}}" data-css-version="{{css_version}}" data-js-version="{{js_version}}">
    <div class="body-inner">
        <div id="mobile-menu">
            <a href='/'><div class="menu-desktop-item home"><img src='{{aws_s3_url}}/icons/logo.png{{css_version}}'></div></a><a class="menu-desktop-main-title" href="/blog"><div class="menu-desktop-item home-title">{{#with i18n}}{{blog}}{{/with}}</div></a>
            <img id="menu-btn-mobile" src="{{aws_s3_url}}/icons/menu.png{{css_version}}">
        </div>
        <div id="mobile-right-menu">
            <div class="close"><img src="{{aws_s3_url}}/icons/delete.png{{css_version}}"></div>
            <!--<div class="select-lang-item"><select class="select-lang">{{#with i18n}}{{{select_lang}}}{{/with}}</select></div>-->
            {{#if is_loginned}}
                <div class="user-item">
                    <div class="user-profile">
                        {{#with user}}
                            <div class="user-profile-left"><img src="{{img}}"></div>
                            <div class="user-profile-right"><div><span class="user-name">{{name}}</span></div><div class="blog-id">@{{blog_id}}</div></div>
                        {{/with}}
                    </div>
                    <div class="user-profile-info"><div id="menu-messages-mobile" class="user-profile-info-item"><img class='emoticon-big-square-img' src='{{aws_s3_url}}/icons/messages-selected.png{{css_version}}'><div class='new' style="display:none;"></div><div class="user-profile-info-title">{{#with i18n}}{{messages}}{{/with}}</div></div><div id="menu-notifications-mobile" class="user-profile-info-item user-profile-info-right-item"><img class='emoticon-big-square-img' src='{{aws_s3_url}}/icons/notifications-selected.png{{css_version}}'><div class='new' style="display:none;"></div><div class="user-profile-info-title">{{#with i18n}}{{notifications}}{{/with}}</div></div><a href='{{profile_url}}'><div class="user-profile-info-item"><img class='emoticon-big-square-img' src='{{aws_s3_url}}/icons/blog-logo-black.png{{css_version}}'><div class="user-profile-info-title">{{#with i18n}}{{blog}}{{/with}}</div></div></a><div id='menu-logout-mobile' class="user-profile-info-item user-profile-info-right-item"><img class='emoticon-big-square-img' src='{{aws_s3_url}}/icons/logout.png{{css_version}}'><div class="user-profile-info-title">{{#with i18n}}{{logout}}{{/with}}</div></div></div>
                </div>
            {{else}}
                <div class="login-item">
                    {{#with i18n}}
                        <div class="login-item-description">{{please_login}}</div>
                        <a href="/login"><div class="login-item-box"><img src="{{aws_s3_url}}/icons/logo.png{{css_version}}" alt="Gleant" title="Gleant"><span>{{login}}</span></div></a>
                        <div class="login-item-bottom">
                            <div class="forgot-password"><a href="/forgot-password"><span>{{forgot_password}}</span></a></div>
                            <div class="register"><a href="/register"><span>{{register}}</span></a></div>
                        </div>
                    {{/with}}
                </div>
            {{/if}}
            <div class="services">{{#with i18n}}<div class="service-title"><span>{{total_service}}</span></div><a href="/news"><div class="service-item"><img src="{{aws_s3_url}}/icons/news-logo2.png{{css_version}}"><div>{{news}}</div></div></a><a href="/debate"><div class="service-item service-right-item"><img src="{{aws_s3_url}}/icons/debate_logo.png{{css_version}}"><div>{{debate}}</div></div></a><a href="/blog"><div class="service-item"><img src="{{aws_s3_url}}/icons/blog-logo.png{{css_version}}"><div>{{blog}}</div></div></a><a href="/"><div class="service-item service-right-item"><img src="{{aws_s3_url}}/icons/search-logo.png{{css_version}}"><div>{{search}}</div></div></a>{{/with}}</div>
        </div>
        <div style="position:absolute;top:0;left:0;right:0;bottom:0;">
            <div class="body-inner-main" style="text-align:center;">
                <div class="menu-space"></div>
                <div id="menu">
                    <div id="menu-inner">
                        <div id="service-list"><!--
                            --><div class="services">{{#with i18n}}<div class="service-title"><span>{{total_service}}</span></div><a href="/news"><div class="service-item"><img src="{{aws_s3_url}}/icons/news-logo2.png{{css_version}}"><div>{{news}}</div></div></a><a href="/debate"><div class="service-item service-right-item"><img src="{{aws_s3_url}}/icons/debate_logo.png{{css_version}}"><div>{{debate}}</div></div></a><a href="/blog"><div class="service-item"><img src="{{aws_s3_url}}/icons/blog-logo.png{{css_version}}"><div>{{blog}}</div></div></a><a href="/"><div class="service-item service-right-item"><img src="{{aws_s3_url}}/icons/search-logo.png{{css_version}}"><div>{{search}}</div></div></a>{{/with}}</div><!--
                        --></div>
                        <a id="menu-desktop-home" href='/'><div class="menu-desktop-item home"><img src='{{aws_s3_url}}/icons/logo.png{{css_version}}'></div></a><a class="menu-desktop-main-title" href="/blog"><div class="menu-desktop-item home-title">{{#with i18n}}{{blog}}{{/with}}</div></a>
                        {{#if is_loginned}}
                            <div id='menu-user-desktop' class='menu-item menu-desktop-item' style="position:relative;">{{#with user}}<img class='emoticon-big-circle-img' src='{{img}}'><span>{{name}}</span>{{/with}}<!--
                            --><div id="desktop-user-menu"><!--
                                --><ul><!--
                                    --><a href='{{profile_url}}'><li id='menu-profile-desktop' class='menu-item'><img class='emoticon-big-square-img' src="{{aws_s3_url}}/icons/blog-logo-black.png{{css_version}}"><span>{{#with i18n}}{{blog}}{{/with}}</span></li></a><!--
                                    --><li id='menu-logout-desktop' class='menu-item'><img class='emoticon-big-square-img' src="{{aws_s3_url}}/icons/logout.png{{css_version}}"><span>{{#with i18n}}{{logout}}{{/with}}</span></li><!--
                                --></ul><!--
                            --></div><!--
                        --></div>
                            <div id='menu-notifications-desktop' class='menu-item menu-desktop-item'><img class='emoticon-big-square-img' src='{{aws_s3_url}}/icons/notifications-selected.png{{css_version}}'><div class='new' style="display:none;"></div></div>
                            <div id='menu-messages-desktop' class='menu-item menu-desktop-item'><img class='emoticon-big-square-img' src='{{aws_s3_url}}/icons/messages-selected.png{{css_version}}'><div class='new' style="display:none;"></div></div>
                        {{else}}
                            <a href='/login' style='float:right;'><div id='menu-login-desktop' class='menu-item menu-desktop-item'>{{#with i18n}}{{login}}{{/with}}</div></a>
                        {{/if}}
                        <div id="btn-services" class="menu-item menu-desktop-item" style="float:right;" {{#with i18n}}alt="{{total_service}}" title="{{total_service}}"{{/with}}><img class='emoticon-big-square-img' src='{{aws_s3_url}}/icons/services.png{{css_version}}'></div>
                        <!--<div id="select-lang-desktop-wrapper" class="menu-desktop-item"><select class="select-lang">{{#with i18n}}{{{select_lang}}}{{/with}}</select></div>-->
                    </div>
                </div>
                <div class="blog-written-wrapper" id="written-wrapper">
                    <div class="article-title" id="blog-title">
                        <span>{{#with i18n}}{{realtime_blog}}{{/with}}</span>
                    </div>
                    <div id="blog-list">
                        {{#each blogs}}<!--
                     --><div class='article-item {{type}}-{{_id}} {{type}}-item' data-print-type='normal'>
                            <div class='article-item-inner' data-link="{{link}}" data-type='{{type}}' data-updated-at='{{updated_at}}'>
                                {{#if is_loginned}}
                                    <div class='article-item-top'>{{blog_name}}</div>
                                {{else}}
                                    <div class='article-item-top'></div>
                                {{/if}}
                                {{#if are_created_at_updated_at_same}}
                                    <div class='article-item-updated-at created-at-small' data-datetime='{{updated_at}}'></div>
                                {{else}}
                                    <div class='article-item-updated-at updated-at-small' data-datetime='{{updated_at}}'></div>
                                {{/if}}
                                <div class='article-item-middle'><a href="{{link}}"><img src='{{thumbnail}}'></a></div>
                                <div class='article-item-counts-wrapper'>
                                    {{#if is_youtube_inserted}}<div class='youtube-inserted'><img src='{{aws_s3_url}}/icons/youtube.png{{css_version}}' title='YouTube' alt='YouTube'></div>{{/if}}
                                    {{#if show_count_view}}<div class='view-counts'><img src='{{aws_s3_url}}/icons/view-selected.png{{css_version}}' title='{{i18n_view}}' alt='{{i18n_view}}'><span>{{count_view}}</span></div>{{/if}}
                                    {{#if show_count_awesome}}<div class='awesome-counts'><img src='{{aws_s3_url}}/icons/awesome-selected.png{{css_version}}' title='{{i18n_awesome}}' alt='{{i18n_awesome}}'><span>{{count_awesome}}</span></div>{{/if}}<!--
                                    {{#if show_count_written_translations}}<div class='written-translation-counts'><img src='{{aws_s3_url}}/icons/write-translation-selected.png{{css_version}}' title='{{i18n_translation}}' alt='{{i18n_translation}}'><span>{{count_written_translations}}</span></div>{{/if}}
                                    {{#if show_count_requested_translations}}<div class='requested-translation-counts'><img src='{{aws_s3_url}}/icons/request-translation-selected.png{{css_version}}' title='{{i18n_translation_request}}' alt='{{i18n_translation_request}}'><span>{{count_requested_translations}}</span></div>{{/if}}
                                 -->{{#if show_count_comments}}<div class='comments-counts'><img src='{{aws_s3_url}}/icons/comments-green.png{{css_version}}' title='{{i18n_comments}}' alt='{{i18n_comments}}'><span>{{count_comments}}</span></div>{{/if}}
                                </div>
                                <div class='article-item-bottom'>{{title}}</div>
                            </div>
                        </div><!--
                    -->{{/each}}
                    </div>
                    <div id="blog-more" class="btn-more list-more"><img class="btn-more-down-14" src="{{aws_s3_url}}/icons/more-down.png{{css_version}}"></div>
                </div>
                <div id="desktop-right">
                    <div id="desktop-realtime-comments" style="margin-top:15px;">
                        <div id="desktop-realtime-comments-title" style="border-top:1px solid #ebebeb;">{{realtime_comments_of_blog}}</div>
                        <ul class="realtime-comments-list"></ul>
                    </div>
                </div>
                <div id="mobile-realtime-comments">
                    <div id="mobile-realtime-comments-title">{{realtime_comments_of_blog}}</div>
                    <ul class="realtime-comments-list"></ul>
                </div>
                <div id="footer">
                    {{#with i18n}}
                        <a href="/announcement"><div class="footer-item">{{announcement}}</div></a>
                        <a href="/policy"><div class="footer-item">{{data_policy}}</div></a>
                        <a href="/terms"><div class="footer-item">{{terms_of_service}}</div></a>
                        <div id="copyright">{{copyright}}</div>
                    {{/with}}
                </div>
            </div>
        </div>
        <div id="share-prompt" class="prompt">
            <div class="prompt-left">
                <div>
                    <div>
                        {{#with i18n}}
                            <div class="prompt-top">
                                <div class="prompt-title"><img class="emoticon-big-img" src="{{aws_s3_url}}/icons/share2-selected.png{{css_version}}"><span>{{share}}</span></div>
                                <div class="prompt-description"></div>
                            </div>
                            <div class="prompt-main">
                                <a class="btn-kakaostory-share"><img id="kakaostory-logo" src="{{aws_s3_url}}/icons/kakaostory-logo.png{{css_version}}"><div id="kakaostory-title">Kakao Story</div></a>
                                <a class="btn-kakaotalk-share" style="display:none;"><img id="kakaotalk-logo" src="{{aws_s3_url}}/icons/kakaotalk-logo.png{{css_version}}"><div id="kakaotalk-title">Kakao Talk</div></a>
                                <a class="btn-twitter-share"><img id="twitter-logo" src="{{aws_s3_url}}/icons/twitter-logo.png{{css_version}}"><div id="twitter-title">Twitter</div></a>
                                <a class="btn-facebook-share"><img id="facebook-logo" src="{{aws_s3_url}}/icons/facebook-logo.png{{css_version}}"><div id="facebook-title">Facebook</div></a>
                                <div id="btn-copy-link">{{copy_url}}</div>
                                <div><div id="text-copy-link"></div></div>
                            </div>
                        {{/with}}
                    </div>
                </div>
            </div>
            <div class="prompt-right"><div class="close"><img src="{{aws_s3_url}}/icons/delete.png{{css_version}}"></div></div>
        </div>
        <div id="request-login-prompt" class="prompt">
            <div class="prompt-left">
                <div>
                    <div>
                        {{#with i18n}}
                            <div class="prompt-main" style="text-align:center;">
                                {{please_login}}
                                <div id="btn-go-to-login-wrapper"><a href="/login"><div id="btn-go-to-login">{{login}}</div></a></div>
                            </div>
                        {{/with}}
                    </div>
                </div>
            </div>
            <div class="prompt-right"><div class="close"><img src="{{aws_s3_url}}/icons/delete.png{{css_version}}"></div></div>
        </div>
        {{#if is_loginned}}
            <div id="write-message-prompt" class="prompt">
                <div class="prompt-left">
                    <div>
                        <div>
                            {{#with i18n}}
                                <div class="prompt-top">
                                    <div class="prompt-title"><img class="emoticon-big-img" src="{{aws_s3_url}}/icons/messages-selected.png{{css_version}}"><span>{{send_message}}</span></div>
                                </div>
                                <div class="prompt-main">
                                    <div class="write-message-top">
                                        <span class="write-message-label">To.</span><img class="write-message-user-img" src="" alt="" title=""><span class="write-message-user-name"></span><span class="write-message-user-blog-id"></span>
                                    </div>
                                    <form id="write-message-form">
                                        <div class="write-message-content-wrapper">
                                            <textarea id="write-message-content"></textarea>
                                        </div>
                                        <div class="btn-career-wrapper">
                                            <input id="write-message-submit" class="btn-career" type="button" value="{{check}}">
                                        </div>
                                    </form>
                                </div>
                            {{/with}}
                        </div>
                    </div>
                </div>
                <div class="prompt-right"><div class="close"><img src="{{aws_s3_url}}/icons/delete.png{{css_version}}"></div></div>
            </div>
            <div id="message-prompt" class="prompt">
                <div class="prompt-left">
                    <div>
                        <div>
                            {{#with i18n}}
                                <div class="prompt-top">
                                    <div class="prompt-title"><a href="/i/messages"><span class="check-all">{{view_all}}<img src="{{aws_s3_url}}/icons/more-right.png{{css_version}}"></span></a><img class="emoticon-big-img" src="{{aws_s3_url}}/icons/messages-selected.png{{css_version}}"><span>{{messages}}</span></div>
                                </div>
                                <div class="prompt-main">
                                    <div class="message-menu"><input class="message-menu-received selected" type='button' value='{{received_messages}}'><input class="message-menu-sent" type='button' value='{{sent_messages}}'></div>
                                    <div class="message-list"></div>
                                    <div class="message-more btn-more"><img class="btn-more-down-14" src="{{aws_s3_url}}/icons/more-down.png{{css_version}}"></div>
                                </div>
                            {{/with}}
                        </div>
                    </div>
                </div>
                <div class="prompt-right"><div class="close"><img src="{{aws_s3_url}}/icons/delete.png{{css_version}}"></div></div>
            </div>
            <div id="notification-prompt" class="prompt">
                <div class="prompt-left">
                    <div>
                        <div>
                            <div class="prompt-top">
                                {{#with i18n}}
                                    <div class="prompt-title"><a href="/i/notifications"><span class="check-all">{{view_all}}<img src="{{aws_s3_url}}/icons/more-right.png{{css_version}}"></span></a><img class="emoticon-big-img" src="{{aws_s3_url}}/icons/notifications-selected.png{{css_version}}"><span>{{notifications}}</span></div>
                                {{/with}}
                            </div>
                            <div class="prompt-main">
                                <div class="notification-list"></div>
                                {{#with i18n}}
                                    <div class="notification-more btn-more"><img class="btn-more-down-14" src="{{aws_s3_url}}/icons/more-down.png{{css_version}}"></div>
                                {{/with}}
                            </div>
                        </div>
                    </div>
                </div>
                <div class="prompt-right"><div class="close"><img src="{{aws_s3_url}}/icons/delete.png{{css_version}}"></div></div>
            </div>
        {{/if}}
        <div id="overlay"></div>
        <div id="overlay2"></div>
        <div id="overlay3"></div>
        <div id="loader"></div>
    </div>
    <script type="text/javascript" src="/js/sub/jquery.min.js{{js_version}}"></script>
    <script type="text/javascript" src="/js/sub/kakao.min.js{{js_version}}"></script>
    <script type="text/javascript" src="/js/sub/clipboard.min.js{{js_version}}"></script>
    <script type="text/javascript" src="/js/sub/i18n_en.js{{js_version}}"></script>
    <script type="text/javascript" src="/js/sub/i18n_ja.js{{js_version}}"></script>
    <script type="text/javascript" src="/js/sub/i18n_ko.js{{js_version}}"></script>
    <script type="text/javascript" src="/js/sub/i18n_zh-Hans.js{{js_version}}"></script>
    <script type="text/javascript" src="/js/sub/i18n.js{{js_version}}"></script>
    <script type="text/javascript" src="/js/sub/global.js{{js_version}}"></script>
    <script type="text/javascript" src="/js/sub/list_methods.js{{js_version}}"></script>
    <script type="text/javascript" src="/js/sub/list.js{{js_version}}"></script>
    <script type="text/javascript" src="/js/main/blog.js{{js_version}}"></script>
    <script type="text/javascript" src="/js/sub/menu.js{{js_version}}"></script>
    <script type="text/javascript" src="/js/sub/simple-profile.js{{js_version}}"></script>
    <script type="text/javascript" src="/js/sub/detailed-career.js{{js_version}}"></script>
    <script type="text/javascript" src="/js/sub/write_form_methods.js{{js_version}}"></script>
    <script type="text/javascript" src="/js/sub/write_form.js{{js_version}}"></script>
    <script type="text/javascript" src="/js/sub/language.js{{js_version}}"></script>
    <script type="text/javascript" src="/js/sub/fixed_information.js{{js_version}}"></script>
    <script type="text/javascript" src="/js/sub/oauth.js{{js_version}}"></script>
</body>
</html>