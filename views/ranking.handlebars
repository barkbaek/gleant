<!DOCTYPE html>
<html itemscope itemtype="http://schema.org/Product" lang="{{lang}}">
<head>
    <link rel="stylesheet" type="text/css" href="/css/main.css{{css_version}}">
    <link rel="stylesheet" type="text/css" href="/css/animate.css{{css_version}}">
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta name="fragment" content="!">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1"/>
    <!--[if lte IE 8]>
    <script src="https://cdn.polyfill.io/v2/polyfill.min.js"></script>
    <![endif]-->
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no, height=device-height">
    <meta name="mobile-web-app-capable" content="yes" />
    <meta name="apple-mobile-web-app-capable" content="yes" />
    <meta name="apple-mobile-web-app-status-bar-style" content="black" />
    <meta http-equiv="content-script-type" content="text/javascript">
    <meta name="title" content="{{title}}">
    <meta name="description" content="{{description}}">
    <meta name="kewords" content="{{keywords}}">
    <meta name="author" content="{{title}}" />
    <meta name="robots" content="index,follow" />
    <meta name="google" content="notranslate">
    <meta itemprop="name" content="{{title}}">
    <meta itemprop="description" content="{{description}}">
    <meta itemprop="image" content="{{image}}">
    <meta name="twitter:card" content="summary">
    <meta name="twitter:site" content="{{twitter_site}}">
    <meta name="twitter:title" content="{{title}}">
    <meta name="twitter:description" content="{{description}}">
    <meta name="twitter:creator" content="{{twitter_site}}">
    <meta name="twitter:image" content="{{image}}">
    <meta property="og:site_name" content="{{site_name}}" />
    <meta property="og:title" content="{{title}}" />
    <meta property="og:type" content="article" />
    <meta property="og:url" content="{{url}}" />
    <meta property="og:image" content="{{image}}" />
    <meta property="og:description" content="{{description}}" />
    <meta property="article:published_time" content="{{date}}" />
    <meta property="article:modified_time" content="{{date}}" />
    <meta property="fb:app_id" content="{{fb_app_id}}" />
    <title>{{title}}</title>
    <link rel="icon" sizes="32x32" href="/favicon.ico{{css_version}}">
    {{#each alternate_list}}
        <link rel="alternate" href="{{url}}" hreflang="{{lang}}" />
    {{/each}}
    <script type="text/javascript" src="/js/sub/check.js{{js_version}}"></script>
</head>
<body data-lang="{{lang}}" data-check="{{is_loginned}}" data-css-version="{{css_version}}" data-js-version="{{js_version}}">
    <div class="body-inner">
        <div style="position:absolute;top:0;left:0;right:0;bottom:0;">
            <div class="body-inner-main" style="text-align:center;">
                <div id="ranking"></div>
                <div id="footer">
                    {{#with i18n}}
                        <a href="/announcement"><div class="footer-item">{{announcement}}</div></a>
                        <a href="/policy"><div class="footer-item">{{data_policy}}</div></a>
                        <a href="/terms"><div class="footer-item">{{terms_of_service}}</div></a>
                        <div id="copyright">{{copyright}}</div>
                    {{/with}}
                </div>
            </div>
        </div>
        <div id="remove-prompt" class="prompt">
            <div class="prompt-left">
                <div>
                    <div>
                        {{#with i18n}}
                            <div class="prompt-main" style="text-align:center;">
                                {{cannot_be_recovered_if_you_delete_it}}<br>{{do_you_want_to_delete_it}}
                                <div id="remove-prompt-btns-wrapper">
                                    <input id="btn-remove-cancel" class="btn-career" type="button" value="{{cancel}}">
                                    <input id="btn-remove-ok" class="btn-career" type="button" value="{{check}}">
                                </div>
                            </div>
                        {{/with}}
                    </div>
                </div>
            </div>
            <div class="prompt-right"><div class="close"><img src="{{aws_s3_url}}/icons/delete.png{{css_version}}"></div></div>
        </div>
        <div id="write-message-prompt" class="prompt">
            <div class="prompt-left">
                <div>
                    <div>
                        {{#with i18n}}
                            <div class="prompt-top">
                                <div class="prompt-title"><img class="emoticon-big-img" src="{{aws_s3_url}}/icons/messages-selected.png{{css_version}}"><span>{{send_message}}</span></div>
                            </div>
                            <div class="prompt-main">
                                <div class="write-message-top">
                                    <span class="write-message-label">To.</span><img class="write-message-user-img" src="" alt="" title=""><span class="write-message-user-name"></span><span class="write-message-user-blog-id"></span>
                                </div>
                                <form id="write-message-form">
                                    <div class="write-message-content-wrapper">
                                        <textarea id="write-message-content"></textarea>
                                    </div>
                                    <div class="btn-career-wrapper">
                                        <input id="write-message-submit" class="btn-career" type="button" value="{{check}}">
                                    </div>
                                </form>
                            </div>
                        {{/with}}
                    </div>
                </div>
            </div>
            <div class="prompt-right"><div class="close"><img src="{{aws_s3_url}}/icons/delete.png{{css_version}}"></div></div>
        </div>
        <div id="message-prompt" class="prompt">
            <div class="prompt-left">
                <div>
                    <div>
                        {{#with i18n}}
                            <div class="prompt-top">
                                <div class="prompt-title"><a href="/i/messages"><span class="check-all">{{view_all}}<img src="{{aws_s3_url}}/icons/more-right.png{{css_version}}"></span></a><img class="emoticon-big-img" src="{{aws_s3_url}}/icons/messages-selected.png{{css_version}}"><span>{{messages}}</span></div>
                            </div>
                            <div class="prompt-main">
                                <div class="message-menu"><input class="message-menu-received selected" type='button' value='{{received_messages}}'><input class="message-menu-sent" type='button' value='{{sent_messages}}'></div>
                                <div class="message-list"></div>
                                <div class="message-more btn-more"><img class="btn-more-down-14" src="{{aws_s3_url}}/icons/more-down.png{{css_version}}"></div>
                            </div>
                        {{/with}}
                    </div>
                </div>
            </div>
            <div class="prompt-right"><div class="close"><img src="{{aws_s3_url}}/icons/delete.png{{css_version}}"></div></div>
        </div>
        <div id="notification-prompt" class="prompt">
            <div class="prompt-left">
                <div>
                    <div>
                        <div class="prompt-top">
                            {{#with i18n}}
                                <div class="prompt-title"><a href="/i/notifications"><span class="check-all">{{view_all}}<img src="{{aws_s3_url}}/icons/more-right.png{{css_version}}"></span></a><img class="emoticon-big-img" src="{{aws_s3_url}}/icons/notifications-selected.png{{css_version}}"><span>{{notifications}}</span></div>
                            {{/with}}
                        </div>
                        <div class="prompt-main">
                            <div class="notification-list"></div>
                            {{#with i18n}}
                                <div class="notification-more btn-more"><img class="btn-more-down-14" src="{{aws_s3_url}}/icons/more-down.png{{css_version}}"></div>
                            {{/with}}
                        </div>
                    </div>
                </div>
            </div>
            <div class="prompt-right"><div class="close"><img src="{{aws_s3_url}}/icons/delete.png{{css_version}}"></div></div>
        </div>
        <div id="overlay"></div>
        <div id="overlay2"></div>
        <div id="loader"></div>
    </div>
    <script type="text/javascript" src="/js/sub/jquery.min.js{{js_version}}"></script>
    <script type="text/javascript" src="/js/sub/kakao.min.js{{js_version}}"></script>
    <script type="text/javascript" src="/js/sub/clipboard.min.js{{js_version}}"></script>
    <script type="text/javascript" src="/js/sub/i18n_en.js{{js_version}}"></script>
    <script type="text/javascript" src="/js/sub/i18n_ja.js{{js_version}}"></script>
    <script type="text/javascript" src="/js/sub/i18n_ko.js{{js_version}}"></script>
    <script type="text/javascript" src="/js/sub/i18n_zh-Hans.js{{js_version}}"></script>
    <script type="text/javascript" src="/js/sub/i18n.js{{js_version}}"></script>
    <script type="text/javascript" src="/js/sub/global.js{{js_version}}"></script>
    <script type="text/javascript" src="/js/main/ranking.js{{js_version}}"></script>
    <script type="text/javascript" src="/js/sub/menu.js{{js_version}}"></script>
    <script type="text/javascript" src="/js/sub/simple-profile.js{{js_version}}"></script>
    <script type="text/javascript" src="/js/sub/list_methods.js{{js_version}}"></script>
    <script type="text/javascript" src="/js/sub/oauth.js{{js_version}}"></script>
</body>
</html>