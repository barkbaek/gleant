                                        <select id="birth-year"></select><select id="birth-month"></select><select id="birth-day"></select>
                                        <div class="btn-career-wrapper"><input class="set-specific-info-cancel btn-career" type="button" value="{{cancel}}"><input class="set-specific-info-submit btn-career" type="submit" value="{{check}}" data-type="birth"></div>
                                    </form>
                                </div>
                                <div id="set-gender-item" class="set-specific-info-item">
                                    <div class="btn-set-specific-info-item">{{gender}}</div>
                                    <form class="set-specific-info-form" data-type="gender" data-sex="{{data_sex}}">
                                        <div class="label-title">{{gender}}</div>
                                        <div class="sex-wrapper">
                                            <div id="male" class="sex">{{male}}</div>
                                            <div id="female" class="sex">{{female}}</div>
                                        </div>
                                        <div class="btn-career-wrapper"><input class="set-specific-info-cancel btn-career" type="button" value="{{cancel}}"><input class="set-specific-info-submit btn-career" type="submit" value="{{check}}" data-type="gender"></div>
                                    </form>
                                </div>
