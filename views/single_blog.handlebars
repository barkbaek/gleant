        <div id="set-specific-info-prompt" class="prompt">
            <div class="prompt-left">
                <div>
                    <div>
                        <div class="prompt-top">
                            {{#with i18n}}
                                <div class="prompt-title"><img class="emoticon-big-img" src="{{aws_s3_url}}/icons/setting.png{{css_version}}"><span>{{setting}}</span></div>
