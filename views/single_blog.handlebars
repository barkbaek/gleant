                                    </form>
                                </div>-->
                                <div id="set-search-engine-item" class="set-specific-info-item">
                                    <div class="btn-set-specific-info-item">{{search_engine_profile_public_setting}}</div>
                                    <form class="set-specific-info-form" data-type="search_engine" data-public-authority="{{data_public_authority}}">
                                        <div class="label-title">{{search_engine_profile_public_setting}}</div>
                                        <div class="search-engine-profile-public-setting-wrapper">
                                            <div id="logged-in-users" class="search-engine-profile-public-setting selected">{{logged_in_users}}</div>
                                            <div id="not-logged-in-users" class="search-engine-profile-public-setting">{{not_logged_in_users}}</div>
                                        </div>
                                        <div class="btn-career-wrapper"><input class="set-specific-info-cancel btn-career" type="button" value="{{cancel}}"><input class="set-specific-info-submit btn-career" type="submit" value="{{check}}" data-type="search_engine"></div>
                                    </form>
                                </div>
                                {{#if is_service_gleant}}
                                    <div id="set-reset-password-item" class="set-specific-info-item">
                                        <div class="btn-set-specific-info-item">{{password_reset}}</div>
                                        <form class="set-specific-info-form" data-type="reset_password">
                                            <div class="label-title">{{password_reset}}</div>
                                            <input id="current-password" type="password" placeholder="{{current_password}}">
                                            <input id="reset-password1" type="password" placeholder="{{new_password}}" style="margin-top:10px;">
                                            <input id="reset-password2" type="password" placeholder="{{new_password}}" style="margin-top:10px;">
                                            <div style="color:#ff7777;font-size:10px;margin-top:5px;line-height:1.5em;">
                                                <div id="password-alphabet-checker" class="format-checker-red">{{at_least_one_alphabet}}</div>
                                                <div id="password-number-checker" class="format-checker-red">{{at_least_one_number}}</div>
                                                <div id="password-special-checker" class="format-checker-red">{{at_least_one_special_character}}</div>
                                                <div id="password-length-checker" class="format-checker-red">{{minimum_length_8}}</div>
                                            </div>
                                            <div class="btn-career-wrapper"><input class="set-specific-info-cancel btn-career" type="button" value="{{cancel}}"><input class="set-specific-info-submit btn-career" type="submit" value="{{check}}" data-type="reset_password"></div>
                                        </form>
                                    </div>
                                {{/if}}
                                <div id="set-withdrawal-item" class="set-specific-info-item">
                                    <div class="btn-set-specific-info-item">{{membership_withdrawal}}</div>
