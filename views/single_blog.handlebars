<!DOCTYPE html>
<html itemscope itemtype="http://schema.org/Product" lang="{{lang}}">
<head>
    <link rel="stylesheet" type="text/css" href="/css/main.css{{css_version}}">
    <link rel="stylesheet" type="text/css" href="/css/animate.css{{css_version}}">
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta name="fragment" content="!">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1"/>
    <!--[if lte IE 8]>
    <script src="https://cdn.polyfill.io/v2/polyfill.min.js"></script>
    <![endif]-->
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no, height=device-height">
    <meta name="mobile-web-app-capable" content="yes" />
    <meta name="apple-mobile-web-app-capable" content="yes" />
    <meta name="apple-mobile-web-app-status-bar-style" content="black" />
    <meta http-equiv="content-script-type" content="text/javascript">
    <meta name="title" content="{{title}}">
    <meta name="description" content="{{description}}">
    <meta name="kewords" content="{{keywords}}">
    <meta name="author" content="{{title}}" />
    <meta name="robots" content="index,follow" />
    <meta name="google" content="notranslate">
    <meta itemprop="name" content="{{title}}">
    <meta itemprop="description" content="{{description}}">
    <meta itemprop="image" content="{{image}}">
    <meta name="twitter:card" content="summary">
    <meta name="twitter:site" content="{{twitter_site}}">
    <meta name="twitter:title" content="{{title}}">
    <meta name="twitter:description" content="{{description}}">
    <meta name="twitter:creator" content="{{twitter_site}}">
    <meta name="twitter:image" content="{{image}}">
    <meta property="og:site_name" content="{{site_name}}" />
    <meta property="og:title" content="{{title}}" />
    <meta property="og:type" content="article" />
    <meta property="og:url" content="{{url}}" />
    <meta property="og:image" content="{{image}}" />
    <meta property="og:description" content="{{description}}" />
    <meta property="article:published_time" content="{{date}}" />
    <meta property="article:modified_time" content="{{date}}" />
    <meta property="fb:app_id" content="{{fb_app_id}}" />
    <title>{{title}}</title>
    <link rel="icon" sizes="32x32" href="/favicon.ico{{css_version}}">
    {{#each alternate_list}}
        <link rel="alternate" href="{{url}}" hreflang="{{lang}}" />
    {{/each}}
    <script type="text/javascript" src="/js/sub/check.js{{js_version}}"></script>
</head>
<body data-lang="{{lang}}" data-check="{{is_loginned}}" data-css-version="{{css_version}}" data-js-version="{{js_version}}">
    <div class="body-inner single-blog">
        <div id="mobile-menu">
            <a href='/blog'><div class="menu-desktop-item home"><img src='{{aws_s3_url}}/icons/blog-logo.png{{css_version}}' style="height:30px;"></div></a><a class="menu-desktop-main-title" href="/blog"><div class="menu-desktop-item home-title">{{#with i18n}}{{blog}}{{/with}}</div></a>
            <img id="menu-btn-mobile" src="{{aws_s3_url}}/icons/menu.png{{css_version}}">
        </div>
        <div id="mobile-right-menu">
            <div class="close"><img src="{{aws_s3_url}}/icons/delete.png{{css_version}}"></div>
            <!--<div class="select-lang-item"><select class="select-lang">{{#with i18n}}{{{select_lang}}}{{/with}}</select></div>-->
            {{#if is_loginned}}
                <div class="user-item">
                    <div class="user-profile">
                        {{#with user}}
                            <div class="user-profile-left"><img src="{{img}}"></div>
                            <div class="user-profile-right"><div><span id="menu-user-name-mobile" class="user-name">{{name}}</span></div><div class="blog-id">@{{blog_id}}</div></div>
                        {{/with}}
                    </div>
                    <div class="user-profile-info"><div id="menu-messages-mobile" class="user-profile-info-item"><img class='emoticon-big-square-img' src='{{aws_s3_url}}/icons/messages-selected.png{{css_version}}'><div class='new' style="display:none;"></div><div class="user-profile-info-title">{{#with i18n}}{{messages}}{{/with}}</div></div><div id="menu-notifications-mobile" class="user-profile-info-item user-profile-info-right-item"><img class='emoticon-big-square-img' src='{{aws_s3_url}}/icons/notifications-selected.png{{css_version}}'><div class='new' style="display:none;"></div><div class="user-profile-info-title">{{#with i18n}}{{notifications}}{{/with}}</div></div><a href='{{profile_url}}'><div class="user-profile-info-item"><img class='emoticon-big-square-img' src='{{aws_s3_url}}/icons/blog-logo-black.png{{css_version}}'><div class="user-profile-info-title">{{#with i18n}}{{blog}}{{/with}}</div></div></a><div id='menu-logout-mobile' class="user-profile-info-item user-profile-info-right-item"><img class='emoticon-big-square-img' src='{{aws_s3_url}}/icons/logout.png{{css_version}}'><div class="user-profile-info-title">{{#with i18n}}{{logout}}{{/with}}</div></div></div>
                </div>
            {{else}}
                <div class="login-item">
                    {{#with i18n}}
                        <div class="login-item-description">{{please_login}}</div>
                        <a href="/login"><div class="login-item-box"><img src="{{aws_s3_url}}/icons/logo.png{{css_version}}" alt="Gleant" title="Gleant"><span>{{login}}</span></div></a>
                        <div class="login-item-bottom">
                            <div class="forgot-password"><a href="/forgot-password"><span>{{forgot_password}}</span></a></div>
                            <div class="register"><a href="/register"><span>{{register}}</span></a></div>
                        </div>
                    {{/with}}
                </div>
            {{/if}}
            <div class="services">{{#with i18n}}<div class="service-title"><span>{{total_service}}</span></div><a href="/news"><div class="service-item"><img src="{{aws_s3_url}}/icons/news-logo2.png{{css_version}}"><div>{{news}}</div></div></a><a href="/debate"><div class="service-item service-right-item"><img src="{{aws_s3_url}}/icons/debate_logo.png{{css_version}}"><div>{{debate}}</div></div></a><a href="/blog"><div class="service-item"><img src="{{aws_s3_url}}/icons/blog-logo.png{{css_version}}"><div>{{blog}}</div></div></a><a href="/"><div class="service-item service-right-item"><img src="{{aws_s3_url}}/icons/search-logo.png{{css_version}}"><div>{{search}}</div></div></a>{{/with}}</div>
        </div>
        <div style="position:absolute;top:0;left:0;right:0;bottom:0;">
            <div class="body-inner-main" style="text-align:center;">
                <div id="menu">
                    <div id="menu-inner" style="max-width:1180px;">
                        <div id="service-list"><!--
                            --><div class="services">{{#with i18n}}<div class="service-title"><span>{{total_service}}</span></div><a href="/news"><div class="service-item"><img src="{{aws_s3_url}}/icons/news-logo2.png{{css_version}}"><div>{{news}}</div></div></a><a href="/debate"><div class="service-item service-right-item"><img src="{{aws_s3_url}}/icons/debate_logo.png{{css_version}}"><div>{{debate}}</div></div></a><a href="/blog"><div class="service-item"><img src="{{aws_s3_url}}/icons/blog-logo.png{{css_version}}"><div>{{blog}}</div></div></a><a href="/"><div class="service-item service-right-item"><img src="{{aws_s3_url}}/icons/search-logo.png{{css_version}}"><div>{{search}}</div></div></a>{{/with}}</div><!--
                     --></div>
                        <a id="menu-desktop-home" href='/blog'><div class="menu-desktop-item home"><img src='{{aws_s3_url}}/icons/blog-logo.png{{css_version}}' style="height:30px;"></div></a><a class="menu-desktop-main-title" href="/blog"><div class="menu-desktop-item home-title">{{#with i18n}}{{blog}}{{/with}}</div></a>
                        {{#if is_loginned}}
                            <div id='menu-user-desktop' class='menu-item menu-desktop-item' style="position:relative;">{{#with user}}<img class='emoticon-big-circle-img' src='{{img}}'><span>{{name}}</span>{{/with}}<!--
                            --><div id="desktop-user-menu"><!--
                                --><ul><!--
                                    --><a href='{{profile_url}}'><li id='menu-profile-desktop' class='menu-item'><img class='emoticon-big-square-img' src="{{aws_s3_url}}/icons/blog-logo-black.png{{css_version}}"><span>{{#with i18n}}{{blog}}{{/with}}</span></li></a><!--
                                    --><li id='menu-logout-desktop' class='menu-item'><img class='emoticon-big-square-img' src="{{aws_s3_url}}/icons/logout.png{{css_version}}"><span>{{#with i18n}}{{logout}}{{/with}}</span></li><!--
                                --></ul><!--
                            --></div><!--
                        --></div>
                            <div id='menu-notifications-desktop' class='menu-item menu-desktop-item'><img class='emoticon-big-square-img' src='{{aws_s3_url}}/icons/notifications-selected.png{{css_version}}'><div class='new' style="display:none;"></div></div>
                            <div id='menu-messages-desktop' class='menu-item menu-desktop-item'><img class='emoticon-big-square-img' src='{{aws_s3_url}}/icons/messages-selected.png{{css_version}}'><div class='new' style="display:none;"></div></div>
                        {{else}}
                            <a href='/login' style='float:right;'><div id='menu-login-desktop' class='menu-item menu-desktop-item'>{{#with i18n}}{{login}}{{/with}}</div></a>
                        {{/if}}
                        <div id="btn-services" class="menu-item menu-desktop-item" style="float:right;" {{#with i18n}}alt="{{total_service}}" title="{{total_service}}"{{/with}}><img class='emoticon-big-square-img' src='{{aws_s3_url}}/icons/services.png{{css_version}}'></div>
                        <!--<div id="select-lang-desktop-wrapper" class="menu-desktop-item"><select class="select-lang">{{#with i18n}}{{{select_lang}}}{{/with}}</select></div>-->
                    </div>
                </div>
                {{#with profile_info}}
                <div id='profile'>
                    <div id="blog-info">
                        <div id="blog-count-view"><div id="blog-count-view-left">{{#with i18n}}{{today}}{{/with}} <span class="counter">{{count_today_view}}</span></div><div id="blog-count-view-right">{{#with i18n}}{{total}}{{/with}} <span class="counter">{{count_total_view}}</span></div></div>
                        <div id="blog-address-wrapper"><img id="copy-blog-address" data-clipboard-text="{{url}}" class="emoticon-x-small-img" src="{{aws_s3_url}}/icons/copy-clipboard.png{{css_version}}" {{#with i18n}}alt="{{copy_url}}" title="{{copy_url}}"{{/with}}><a id="blog-address" href="{{url}}">{{url}}</a></div>
                    </div>
                    {{#if is_loginned}}
                    <div id="blog-name"><a href='/blog/{{blog_id}}'><span id="blog-name-text">{{blog_name}}</span></a>{{#if is_owned}}<span id="edit-blog-name">{{#with i18n}}{{edit}}{{/with}}</span>{{/if}}</div>
                    <div id='profile-top'>
                        <div id='profile-top-left'>
                            <img id='profile-img' src='{{img}}'>
                            {{#if is_owned}}{{#with i18n}}<div class='change-profile-image desktop-only' alt='{{profile_photo_change}}' title='{{profile_photo_change}}'>{{profile_photo_change}}</div><div class='change-profile-image mobile-only' alt='{{profile_photo_change}}' title='{{profile_photo_change}}'>{{change}}</div>{{/with}}{{/if}}
                        </div>
                        <div id='profile-top-right'>
                            <div id='user-name'><span id="name-content">{{name}}</span>{{{verified_profile}}}</div>
                            <div id='blog-id'><a href='/blog/{{blog_id}}'>@{{blog_id}}</a></div>
                            <div id='user-birthdate-gender'><span id='user-birthdate'>{{i18n_birth_date}}</span><span id='user-sex'>({{i18n_sex}})</span></div>
                            <!--<div id="user-languages">{{languages}}</div>-->
                            {{#if is_owned}}<div id='login-type'>{{login_type}}</div>{{/if}}
                            <div id='iq-eq' {{#if is_iq_eq_empty_and_owned}}class='empty'{{/if}}><span>{{iq_eq}}</span>{{#if show_edit_iq_eq}}<span id='edit-iq-eq'>{{#with i18n}}{{edit}}{{/with}}</span>{{/if}}</div>
                            <div id='simple-career' {{#if is_simple_career_empty}}class='empty'{{/if}}><span id="simple-career-content">{{simple_career}}</span>{{#if is_show_simple_career_and_owned}}<span id='show-simple-career'>✔</span>{{/if}}{{#if is_simple_career_not_empty_and_owned}}<span id='edit-simple-career'>{{#with i18n}}{{edit}}{{/with}}</span>{{/if}}</div>
                            <div id='self-introduction' {{#if is_self_introduction_empty}}class='empty'{{/if}}><span id="self-introduction-content">{{self_introduction}}</span>{{#if is_self_introduction_not_empty_and_owned}}<span id='edit-self-introduction'>{{#with i18n}}{{edit}}{{/with}}</span>{{/if}}</div>
                        </div>
                    </div>
                    <div id='profile-middle'>
                        <div id='profile-middle-wrapper'>
                            <div class="user-awesome-counts"><img class="emoticon-x-small-img" src="{{aws_s3_url}}/icons/awesome.png{{css_version}}" title="{{#with i18n}}{{awesome}}{{/with}}" alt="{{#with i18n}}{{awesome}}{{/with}}"><span>{{count_awesome}}</span></div>{{{friend_request_element}}}{{{friends_element}}}{{{invitations_element}}}{{{specific_info_element}}}{{#if is_loginned_not_owned}}<div id='send-message' class='blog-send-message' data-blog-id="{{blog_id}}" data-name="{{name}}" data-img="{{img}}"><img class="emoticon-small-img" src="{{aws_s3_url}}/icons/messages-selected.png{{css_version}}"><span style="vertical-align: middle;">{{#with i18n}}{{send_message}}{{/with}}</span></div>{{/if}}{{#if is_gleantcorp}}<a href="/cms" style="display:inline-block;"><div class="go-cms"><span>CMS</span></div></a>{{/if}}
                        </div>
                    </div>
                    {{else}}
                        <div id="blog-name"><a href='/blog/{{blog_id}}'><span id="blog-name-text">{{blog_id}} {{#with i18n}}{{blog}}{{/with}}</span></a></div>
                    {{/if}}
                    <div id='profile-bottom'>
                        <div id='profile-bottom-left'>
                            <div id='profile-bottom-left-title'><span>{{#with i18n}}{{menu}}{{/with}}</span></div>
                            <ul id="blog-menu"><!--
                             -->{{#each blog_menu}}{{#if is_debate}}<a class="blog-menu-link {{#if is_selected}}selected{{/if}}" href="/blog/{{blog_id}}"><li><span class='feed-title'>{{title}}</span></li></a>{{else}}<a class="blog-menu-link {{#if is_selected}}selected{{/if}}" href="/blog/{{blog_id}}/{{_id}}"><li><span class='feed-title'>{{title}}</span></li></a>{{/if}}{{/each}}<!--
                         --></ul>
                        </div>
                        <div id='profile-bottom-right'>
                            <div id='profile-bottom-right-title' class="blog-menu-title">
                                {{#if is_owned}}
                                    {{#if is_blog}}
                                        <span class="write-blog">
                                            <a href="/blog/{{blog_id}}/write/{{blog_menu_id}}">
                                                {{#with i18n}}<img src="{{aws_s3_url}}/icons/write-blog-selected.png{{css_version}}" title="{{article_writing}}" alt="{{article_writing}}">{{article_writing}}{{/with}}
                                            </a>
                                        </span>
                                    {{/if}}
                                    {{#if is_gallery}}
                                        <span class="add-image-gallery">
                                            {{#with i18n}}<img src="{{aws_s3_url}}/icons/photo.png{{css_version}}" title="{{image_add}}" alt="{{image_add}}"><span class="blog-action-icon-text">{{image_add}}</span>{{/with}}
                                        </span>
                                    {{/if}}
                                {{/if}}
                                {{#if is_gallery}}
                                    <span class="btn-gallery-align-flex">
                                        {{#with i18n}}<img src="{{aws_s3_url}}/icons/align-flex.png{{css_version}}" title="{{original_alignment}}" alt="{{original_alignment}}"><span class="blog-action-icon-text">{{original_alignment}}</span>{{/with}}
                                    </span>
                                    <span class="btn-gallery-align-square selected">
                                        {{#with i18n}}<img src="{{aws_s3_url}}/icons/align-square-selected.png{{css_version}}" title="{{thumbnail_alignment}}" alt="{{thumbnail_alignment}}"><span class="blog-action-icon-text">{{thumbnail_alignment}}</span>{{/with}}
                                    </span>
                                {{/if}}
                                {{selected_blog_menu_title}}
                            </div>
                            <div id='profile-bottom-right-content'>
                                {{#if is_debate_selected}}
                                    <div id="blog-debate">
                                        <div id="blog-debate-first-floor">
                                            <a id="blog-debate-first-floor-left" class="{{#if is_agenda_selected}}selected{{/if}}" href="/blog/{{blog_id}}"><div>{{#with i18n}}{{agenda}}{{/with}}</div></a>
                                            <a id="blog-debate-first-floor-middle" class="{{#if is_opinion_selected}}selected{{/if}}" href="/blog/{{blog_id}}/opinion"><div>{{#with i18n}}{{opinion}}{{/with}}</div></a>
                                            <!--<a id="blog-debate-first-floor-right" class="{{#if is_translation_selected}}selected{{/if}}" href="/blog/{{blog_id}}/translation"><div>{{#with i18n}}{{translation}}{{/with}}</div></a>-->
                                        </div>
                                        {{#if is_agenda_selected}}
                                            <div id="blog-debate-second-floor">
                                                <a class="{{debate_in_progress_class}}" href="/blog/{{blog_id}}"><div>{{#with i18n}}{{in_progress}}{{/with}}</div></a>
                                                <a class="{{debate_scheduled_class}}" href="/blog/{{blog_id}}/scheduled-agenda"><div>{{#with i18n}}{{scheduled}}{{/with}}</div></a>
                                                <a class="{{debate_finished_class}}" href="/blog/{{blog_id}}/finished-agenda"><div>{{#with i18n}}{{finished}}{{/with}}</div></a>
                                                <a class="{{debate_unlimited_class}}" href="/blog/{{blog_id}}/unlimited-agenda"><div>{{#with i18n}}{{unlimited}}{{/with}}</div></a>
                                            </div>
                                        {{/if}}
                                    </div>
                                {{/if}}
                                {{#if is_employment_selected}}
                                    <div id="blog-employment">
                                        <div id="blog-employment-first-floor">
                                            <a id="blog-employment-first-floor-right" class="{{#if is_apply_now_selected}}selected{{/if}}" href="/blog/{{blog_id}}/employment"><div>{{#with i18n}}{{apply_now}}{{/with}}</div></a>
                                            <a id="blog-employment-first-floor-left" class="{{#if is_hire_me_selected}}selected{{/if}}" href="/blog/{{blog_id}}/hire-me"><div>{{#with i18n}}{{hire_me}}{{/with}}</div></a>
                                        </div>
                                    </div>
                                {{/if}}
                                {{#if is_participation_selected}}
                                    <div id="blog-participation">
                                        <div id="blog-participation-top-floor">
                                            <a id="blog-participation-top-floor-single" class="{{#if is_debate_participation_selected}}selected{{/if}}" href="/blog/{{blog_id}}/participation"><div>{{#with i18n}}{{debate}}{{/with}}</div></a>
                                        </div>
                                        {{#if is_debate_participation_selected}}
                                            <div id="blog-participation-debate-first-floor">
                                                <a class="{{participation_in_progress_class}}" href="/blog/{{blog_id}}/participation"><div>{{#with i18n}}{{in_progress}}{{/with}}</div></a>
                                                <a class="{{participation_scheduled_class}}" href="/blog/{{blog_id}}/scheduled-agenda-participation"><div>{{#with i18n}}{{scheduled}}{{/with}}</div></a>
                                                <a class="{{participation_finished_class}}" href="/blog/{{blog_id}}/finished-agenda-participation"><div>{{#with i18n}}{{finished}}{{/with}}</div></a>
                                                <a class="{{participation_unlimited_class}}" href="/blog/{{blog_id}}/unlimited-agenda-participation"><div>{{#with i18n}}{{unlimited}}{{/with}}</div></a>
                                            </div>
                                        {{/if}}
                                        {{#if is_employment_participation_selected}}
                                            <div id="blog-participation-employment-first-floor">
                                                <a id="blog-participation-employment-first-floor-left" class="{{#if is_apply_now_selected}}selected{{/if}}" href="/blog/{{blog_id}}/apply-now-participation"><div>{{#with i18n}}{{apply_now}}{{/with}}</div></a>
                                                <a id="blog-participation-employment-first-floor-right" class="{{#if is_hire_me_selected}}selected{{/if}}" href="/blog/{{blog_id}}/hire-me-participation"><div>{{#with i18n}}{{hire_me}}{{/with}}</div></a>
                                            </div>
                                        {{/if}}
                                    </div>
                                {{/if}}
                                {{#if is_subscription_selected}}
                                    <div id="blog-subscription">
                                        <div id="blog-subscription-top-floor">
                                            <a id="blog-subscription-top2-floor-left" class="{{#if is_debate_subscription_selected}}selected{{/if}}" href="/blog/{{blog_id}}/subscription"><div>{{#with i18n}}{{debate}}{{/with}}</div></a>
                                            <a id="blog-subscription-top2-floor-right" class="{{#if is_blog_subscription_selected}}selected{{/if}}" href="/blog/{{blog_id}}/blog-subscription"><div>{{#with i18n}}{{blog}}{{/with}}</div></a>
                                        </div>
                                        {{#if is_debate_subscription_selected}}
                                            <div id="blog-subscription-debate-first-floor">
                                                <a id="blog-subscription-debate-first-floor-left" class="{{#if is_agenda_selected}}selected{{/if}}" href="/blog/{{blog_id}}/subscription"><div>{{#with i18n}}{{agenda}}{{/with}}</div></a>
                                                <a id="blog-subscription-debate-first-floor-middle" class="{{#if is_opinion_selected}}selected{{/if}}" href="/blog/{{blog_id}}/opinion-subscription"><div>{{#with i18n}}{{opinion}}{{/with}}</div></a>
                                                <!--<a id="blog-subscription-debate-first-floor-right" class="{{#if is_translation_selected}}selected{{/if}}" href="/blog/{{blog_id}}/translation-subscription"><div>{{#with i18n}}{{translation}}{{/with}}</div></a>-->
                                            </div>
                                        {{/if}}
                                        {{#if is_employment_subscription_selected}}
                                            <div id="blog-subscription-employment-first-floor">
                                                <a id="blog-subscription-employment-first-floor-left" class="{{#if is_apply_now_selected}}selected{{/if}}" href="/blog/{{blog_id}}/apply-now-subscription"><div>{{#with i18n}}{{apply_now}}{{/with}}</div></a>
                                                <a id="blog-subscription-employment-first-floor-right" class="{{#if is_hire_me_selected}}selected{{/if}}" href="/blog/{{blog_id}}/hire-me-subscription"><div>{{#with i18n}}{{hire_me}}{{/with}}</div></a>
                                            </div>
                                        {{/if}}
                                        {{#if is_agenda_selected}}
                                            <div id="blog-subscription-debate-second-floor">
                                                <a class="{{subscription_in_progress_class}}" href="/blog/{{blog_id}}/subscription"><div>{{#with i18n}}{{in_progress}}{{/with}}</div></a>
                                                <a class="{{subscription_scheduled_class}}" href="/blog/{{blog_id}}/scheduled-agenda-subscription"><div>{{#with i18n}}{{scheduled}}{{/with}}</div></a>
                                                <a class="{{subscription_finished_class}}" href="/blog/{{blog_id}}/finished-agenda-subscription"><div>{{#with i18n}}{{finished}}{{/with}}</div></a>
                                                <a class="{{subscription_unlimited_class}}" href="/blog/{{blog_id}}/unlimited-agenda-subscription"><div>{{#with i18n}}{{unlimited}}{{/with}}</div></a>
                                            </div>
                                        {{/if}}
                                    </div>
                                {{/if}}
                                {{#if is_guestbook}}
                                    {{#if is_loginned}}
                                        <div class="write-guestbook-wrapper">
                                            <form class="write-guestbook-form" data-blog-id="{{blog_id}}">
                                                <div class="write-guestbook-top"></div>
                                                <div class="write-guestbook-middle">
                                                    <div class="write-guestbook-middle-left">
                                                        {{#with user}}
                                                            <img class="write-guestbook-img" src="{{img}}">
                                                            <div class="write-guestbook-name">{{name}}</div>
                                                        {{/with}}
                                                    </div>
                                                    <div class="write-guestbook-middle-right">
                                                        <textarea class="write-guestbook-textarea"></textarea>
                                                    </div>
                                                </div>
                                                <div class="write-guestbook-bottom">
                                                    {{#with i18n}}
                                                        <label>
                                                            <img class="emoticon-x-small-img guestbook-secret-img" src="{{aws_s3_url}}/icons/secret.png"><span class="write-guestbook-secret-text">{{secret}}</span><input class="write-guestbook-secret" type="checkbox">
                                                        </label>
                                                        <input id='write-guestbook-ok' class='btn-career' type='button' value='{{check}}'>
                                                    {{/with}}
                                                </div>
                                            </form>
                                        </div>
                                    {{/if}}
                                {{/if}}
                                {{#unless is_write}}
                                    {{#if is_list}}
                                        {{#if is_gallery}}
                                            <div id="blog-image-list" style="display:none;">
                                                {{{multi_images}}}
