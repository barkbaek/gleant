                                    <form class="set-specific-info-form" data-type="name" data-name="{{data_name}}">
                                        <label for="name">{{real_name}}</label>
                                        <input id="name" type="text" placeholder="">
                                        <div class="btn-career-wrapper"><input class="set-specific-info-cancel btn-career" type="button" value="{{cancel}}"><input class="set-specific-info-submit btn-career" type="submit" value="{{check}}" data-type="name"></div>
                                    </form>
                                </div>
                                <div id="set-birth-item" class="set-specific-info-item">
                                    <div class="btn-set-specific-info-item">{{birth_date}}</div>
                                    <form class="set-specific-info-form" data-type="birth" data-birth-year="{{data_birth_year}}" data-birth-month="{{data_birth_month}}" data-birth-day="{{data_birth_day}}">
